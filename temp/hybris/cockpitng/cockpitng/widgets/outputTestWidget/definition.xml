<widget-definition xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:noNamespaceSchemaLocation="http://www.hybris.com/schema/cockpitng/widget-definition.xsd"
	id="com.hybris.cockpitng.outputtestwidget">

	<name>OutputTestWidget</name>
	<description>Shows all variables and their values of incoming objects.</description>
	<keywords>
		<keyword>Breadboard</keyword>
	</keywords>
	<sockets>
		<input type="java.lang.Object" id="genericInput"/>
		<input type="java.lang.Object" id="collectionInput" multiplicity="Collection"/>

	</sockets>
	<controller class="com.hybris.cockpitng.widgets.controller.OutputTestWidgetController"/>
</widget-definition>
