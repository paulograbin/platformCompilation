<widget-definition
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:noNamespaceSchemaLocation="http://www.hybris.com/schema/cockpitng/widget-definition.xsd"
	id="com.hybris.cockpitng.inputtestwidget" >

	<name>InputTestWidget</name>
	<description>Contains a groovy console that allows simulating any kind of output.</description>
	<keywords>
		<keyword>Breadboard</keyword>
	</keywords>
	<sockets>
		<output type="&lt;T&gt;" id="listoutput" multiplicity="List"/>
		<output type="&lt;T&gt;" id="collectionoutput" multiplicity="Collection"/>
		<output type="&lt;T&gt;" id="setoutput" multiplicity="Set"/>
		<output type="&lt;T&gt;" id="singleoutput" />
	</sockets>
	<controller class="com.hybris.cockpitng.widgets.controller.InputTestController"/>

</widget-definition>
