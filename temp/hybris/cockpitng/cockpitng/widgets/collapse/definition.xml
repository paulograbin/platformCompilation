<widget-definition id="com.hybris.cockpitng.widgets.common.collapse"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:noNamespaceSchemaLocation="http://www.hybris.com/schema/cockpitng/widget-definition.xsd">
	<name>Collapse Widget</name>
	<description>Collapse widget is used to control the sidebar collapse or expansion.</description>
	<defaultTitle>Collapse Widget</defaultTitle>
	<author>hybris</author>
	<version>1.0</version>

	<sockets>
		<input id="setCollapseStatus" type="java.lang.Boolean" />
		<output id="collapseStatus" type="java.lang.Boolean" />
	</sockets>

	<settings>
	</settings>

	<view src="collapse.zul" />

	<controller
		class="com.hybris.cockpitng.widgets.common.collapse.CollapseController" />

	<keywords>
		<keyword>Collapse</keyword>
	</keywords>

</widget-definition>
