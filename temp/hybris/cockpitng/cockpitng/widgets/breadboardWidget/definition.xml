<widget-definition xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:noNamespaceSchemaLocation="http://www.hybris.com/schema/cockpitng/widget-definition.xsd"
	id="com.hybris.cockpitng.breadboardwidget" >

	<name>Breadboard</name>
	<description>Widget for testing widgets.</description>
	<keywords>
		<keyword>Breadboard</keyword>
	</keywords>
	<sockets>
		<input id="singleInputLogger" visibility="invisible"/>
		<input id="collectionInputLogger" visibility="invisible" multiplicity="Collection"/>
		<input id="singleOutputLogger" visibility="invisible"/>
		<input id="collectionOutputLogger" visibility="invisible" multiplicity="Collection"/>
		<input id="selectWidgetDefinition" type="java.lang.String"/>
	</sockets>
	<controller class="com.hybris.cockpitng.widgets.controller.BreadboardController"/>

</widget-definition>
