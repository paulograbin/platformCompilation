<widget-definition xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
						 xsi:noNamespaceSchemaLocation="http://www.hybris.com/schema/cockpitng/widget-definition.xsd"
						 id="com.hybris.cockpitng.eventproducer">
	<name>Event Producer</name>
	<description>Emits events with the supplied data.</description>
	<author>hybris</author>
	<version>0.1</version>

	<view src="none"/>
	<controller class="com.hybris.cockpitng.widgets.controller.EventProducerWidgetController"/>

	<sockets>
		<input id="genericInput"/>
	</sockets>

	<settings>
		<setting key="eventName" type="String" default-value="default"/>
	</settings>

	<keywords>
		<keyword>Adapters</keyword>
	</keywords>
</widget-definition>
