<editor-definition id="com.hybris.cockpitng.editor.defaultrange"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:noNamespaceSchemaLocation="http://www.hybris.com/schema/cockpitng/editor-definition.xsd">

	<name>Default Range Editor</name>
	<description>Range editor is used to render a pair of editors interpreted as a from-to value.</description>
	<author>hybris</author>
	<version>0.1</version>

	<type>^Range\((.*)\)$</type>
	<editorClassName>com.hybris.cockpitng.editor.defaultrange.DefaultRangeEditor</editorClassName>
	<settings>
		<setting key="focusComponentId" type="String" default-value="startEditor"/>
	</settings>
</editor-definition>
