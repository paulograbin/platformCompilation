<action-definition id="com.hybris.cockpitng.action.logoaction"
						 xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
						 xsi:noNamespaceSchemaLocation="http://www.hybris.com/schema/cockpitng/action-definition.xsd">
	<name>SAP CX Home</name>
	<description>Link to static web page</description>
	<author>hybris</author>
	<version>0.1</version>

	<actionClassName>com.hybris.cockpitng.actions.LogoAction</actionClassName>
	<customRendererClassName>com.hybris.cockpitng.actions.LogoAction</customRendererClassName>
	
	<iconUri>pixelate</iconUri>

	<settings>
		<setting key="imageUrl" default-value="" type="java.lang.String"/>
		<setting key="hoverImageUrl" default-value="" type="java.lang.String"/>
		<setting key="target" default-value="_blank" type="java.lang.String"/>
		<setting key="hyperlink" default-value="https://cx.sap.com" type="java.lang.String"/>
		<setting key="buttonsclass" type="java.lang.String" default-value="yw-logo-button"/>
	</settings>
</action-definition>
