<action-definition xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
				   id="com.hybris.cockpitng.action.deeplink.copy.item"
				   xsi:noNamespaceSchemaLocation="http://www.hybris.com/schema/cockpitng/action-definition.xsd">
	<name>Copy Item DeepLink</name>
	<description>Action providing default implementation of copying deeplink of selected item.</description>
	<author>hybris</author>
	<version>1.0</version>
	<sockets>
		<output id="CopyItemDeepLink" type="java.lang.Object" />
	</sockets>
	<settings>
		<setting key="deepLinkConfigCtx" default-value="deep-link" type="String" />
	</settings>
	<iconUri>action</iconUri>
	<actionClassName>com.hybris.cockpitng.actions.deeplink.copy.ItemLinkCopyAction</actionClassName>
</action-definition>
