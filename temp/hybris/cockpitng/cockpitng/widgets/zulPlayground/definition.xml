<widget-definition xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:noNamespaceSchemaLocation="http://www.hybris.com/schema/cockpitng/widget-definition.xsd" id="com.hybris.cockpitng.zulplayground">
	<name>ZUL Playground</name>
	<description>Widget for dynamically testing/displaying ZUL content</description>
	<defaultTitle>ZUL Playground</defaultTitle>
	<author>hybris</author>
	<version>0.1</version>

	<sockets>
		<input id="zulContent" type="java.lang.String"/>
		<output id="injectTemplate" type="java.lang.String"/>
	</sockets>

	<keywords>
		<keyword>Test</keyword>
	</keywords>
</widget-definition>
