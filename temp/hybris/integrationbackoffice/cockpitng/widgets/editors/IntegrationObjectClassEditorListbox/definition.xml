<widget-definition xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
                   xsi:noNamespaceSchemaLocation="http://www.hybris.com/schema/cockpitng/widget-definition.xsd"
                   id="de.hybris.platform.integrationbackoffice.widgets.IntegrationObjectClassEditorListbox">
    <name>Integration Object Class Editor Listbox</name>
    <description>Listbox to display the attributes of a Class</description>
    <defaultTitle>Integration Object Class Editor Listbox</defaultTitle>
    <author>SAP</author>
    <version>1.0</version>
    <keywords>
        <keyword>IntegrationObjectClassEditorListbox</keyword>
    </keywords>
    <view src="IntegrationObjectClassEditorListbox.zul"/>
    <controller
            class="de.hybris.platform.integrationbackoffice.widgets.modeling.controllers.IntegrationObjectClassEditorListboxController"/>
    <sockets>
        <input id="selectedIntegrationClass"
               type="de.hybris.platform.integrationbackoffice.widgets.modeling.data.dto.IntegrationClassMapKeyDTO"/>
        <input id="clearListBox" type="java.lang.Object"/>
        <output id="checkForStructuredType"
                type="de.hybris.platform.integrationbackoffice.widgets.modeling.data.dto.ListItemClassAttributeDTO"/>
        <output id="enableSaveButton" type="java.lang.Boolean"/>
    </sockets>
</widget-definition>
