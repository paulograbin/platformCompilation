#
# Import the English CMS content for the Apparel Site UK
#

$contentCatalog=apparel-ukContentCatalog
$contentCV=catalogVersion(CatalogVersion.catalog(Catalog.id[default=$contentCatalog]),CatalogVersion.version[default=Staged])[default=$contentCatalog:Staged]

# Language
$lang=ja


    
# Content Pages
UPDATE ContentPage;$contentCV[unique=true];uid[unique=true];title[lang=$lang]
 ;;add-edit-address;"住所を追加/編集"
 ;;address-book;"アドレス帳"
 ;;cartPage;"ショッピングバッグ"
 ;;checkout-login;"チェックアウトに進む"
 ;;close-account;"アカウントページを閉じる"
 ;;consents;"同意管理ページ"
 ;;faq;"FAQ"
 ;;homepage;"ホームページ"
 ;;importCSVSavedCartPage;"保存されたカートをインポート"
 ;;login;"ログイン"
 ;;multiStepCheckoutSummaryPage;"チェックアウト"
 ;;notFound;"見つかりません"
 ;;order;"注文詳細"
 ;;orderConfirmationPage;"注文確認"
 ;;orderExpired;"注文期限切れページ"
 ;;orders;"注文履歴"
 ;;payment-details;"支払詳細"
 ;;quickOrderPage;"簡易注文ページ"
 ;;saved-carts;"保存されたカート"
 ;;savedCartDetailsPage;"保存されたカートの詳細"
 ;;search;"検索結果ページ"
 ;;searchEmpty;"結果なし"
 ;;storefinderPage;"店舗検索"
 ;;termsAndConditions;"諸条件"
 ;;update-email;"電子メールの更新ページ"
 ;;update-profile;"プロフィールの更新ページ"
 ;;updatePassword;"忘れたパスワードの更新"

        
# Category Pages
UPDATE CategoryPage;$contentCV[unique=true];uid[unique=true];title[lang=$lang]
 ;;BrandsCategoryPage;"ブランドカテゴリページ"
 ;;ShirtsCategoryPage;"シャツカテゴリページ"
 ;;SnowCategoryPage;"スノーカテゴリページ"
 ;;StreetCategoryPage;"スノーカテゴリページ"
 ;;SunglassesCategoryPage;"サングラスカテゴリページ"
 ;;SurfCategoryPage;"サーフカテゴリページ"
 ;;category;"デフォルトカテゴリページ"
 ;;productGrid;"製品グリッド"
 ;;productList;"商品リスト"

        
# Product Pages
UPDATE ProductPage;$contentCV[unique=true];uid[unique=true];title[lang=$lang]
 ;;productDetails;"商品詳細"

    