#
# Import the English CMS content for the Apparel Site UK
#

$contentCatalog=apparel-ukContentCatalog
$contentCV=catalogVersion(CatalogVersion.catalog(Catalog.id[default=$contentCatalog]),CatalogVersion.version[default=Staged])[default=$contentCatalog:Staged]

# Language
$lang=zh


    
# Content Pages
UPDATE ContentPage;$contentCV[unique=true];uid[unique=true];title[lang=$lang]
 ;;add-edit-address;"添加/编辑地址"
 ;;address-book;"地址簿"
 ;;cartPage;"您的购物袋"
 ;;checkout-login;"继续结账"
 ;;close-account;"关闭帐户页面"
 ;;consents;"许可管理页面"
 ;;faq;"常见问题"
 ;;homepage;"主页"
 ;;importCSVSavedCartPage;"导入已保存购物车"
 ;;login;"登录"
 ;;multiStepCheckoutSummaryPage;"结账"
 ;;notFound;"未找到"
 ;;order;"订单详细信息"
 ;;orderConfirmationPage;"订单确认"
 ;;orderExpired;"订单过期页面"
 ;;orders;"订单历史记录"
 ;;payment-details;"付款详细信息"
 ;;quickOrderPage;"快速订单页面"
 ;;saved-carts;"已保存购物车"
 ;;savedCartDetailsPage;"已保存购物车详细信息"
 ;;search;"搜索结果页面"
 ;;searchEmpty;"无结果"
 ;;storefinderPage;"店铺搜索器"
 ;;termsAndConditions;"条款和条件"
 ;;update-email;"更新电子邮件页面"
 ;;update-profile;"更新个人资料页面"
 ;;updatePassword;"更新忘记的密码"

        
# Category Pages
UPDATE CategoryPage;$contentCV[unique=true];uid[unique=true];title[lang=$lang]
 ;;BrandsCategoryPage;"品牌类别页面"
 ;;ShirtsCategoryPage;"衬衫类别页面"
 ;;SnowCategoryPage;"冰雪类别页面"
 ;;StreetCategoryPage;"街道类别页面"
 ;;SunglassesCategoryPage;"太阳镜类别页面"
 ;;SurfCategoryPage;"冲浪类别页面"
 ;;category;"默认类别页面"
 ;;productGrid;"产品网格"
 ;;productList;"产品列表"

        
# Product Pages
UPDATE ProductPage;$contentCV[unique=true];uid[unique=true];title[lang=$lang]
 ;;productDetails;"产品详细信息"

    